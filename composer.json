{
    "name": "knplabs/knp-snappy",
    "type": "library",
    "description": "PHP library allowing thumbnail, snapshot or PDF generation from a url or a html page. Wrapper for wkhtmltopdf/wkhtmltoimage.",
    "keywords": ["pdf", "thumbnail", "snapshot", "knplabs", "knp", "wkhtmltopdf"],
    "homepage": "http://github.com/KnpLabs/snappy",
    "license": "MIT",
    "authors": [
        {
            "name": "KNP Labs Team",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://github.com/KnpLabs/snappy/contributors"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/process": "^5.4||^6.2||^7.0",
        "psr/log": "^2.0||^3.0",
        "symfony/http-client": "^6.2||7.0",
        "psr/http-message": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "KnpLabs\\Snappy\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
