{
    "name": "snappy/dompdf",
    "require": {
        "php": ">=8.1",
        "dompdf/dompdf": "^2.0",
        "psr/http-message": "^2.0",
        "psr/http-factory": "^1.0",
        "knplabs/snappy-core": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "KnpLabs\\Snappy\\Backend\\Dompdf\\": "./"
        },
        "exclude-from-classmap": ["/spec/"]
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\KnpLabs\\Snappy\\Backend\\Dompdf\\": "spec/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^7.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/KnpLabs/snappy-core"
        }
    ]
}
